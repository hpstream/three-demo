{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": false,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "lib": ["esnext", "dom"],
    "typeRoots": [],
    "types": ["ammojs-typed"],
      
  },
  "include": ["src","pages","types"],
  "references": [{
    "path": "./tsconfig.node.json"
  }]
}